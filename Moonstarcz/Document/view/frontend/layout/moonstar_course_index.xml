    <body>
        <referenceBlock name="page.main.title" remove="true"/>
        <referenceContainer name="content">
            <block class="Moonstarcz\Document\Block\Course\ListCourse" name="course.list"
                   template="Moonstarcz_Document::course/list.phtml" cacheable="false" />
        </referenceContainer>
    </body>
</page>
